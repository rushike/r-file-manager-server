{
  "name": "r-file-manager-server",
  "version": "1.0.0",
  "description": "File Manager with Mutter, ExpressJs",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "start": "ts-node \"./src/index.ts\"",
    "test": "mocha -r ts-node/register --watch-extension ts 'tests/**/*.test.ts'",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test"
  },
  "author": "rushike",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/deasync": "^0.1.1",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.1.1",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@types/multer": "^1.4.4",
    "@types/object-hash": "^1.3.3",
    "body-parser": "^1.19.0",
    "deasync": "^0.1.20",
    "directory-tree": "^2.2.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.2",
    "multer": "^1.4.2",
    "object-hash": "^2.0.3",
    "path": "^0.12.7",
    "pg": "^8.3.3",
    "sqlite3": "^5.0.0",
    "typeorm": "^0.2.28"
  }
}
